    <input type="number" name="phone1" placeholder="Phone 1" class="input" autocomplete="false"><br><br>
    <input type="number" name="phone2" placeholder="Phone 2" class="input" autocomplete="false"><br><br>
    <input type="text" name="email" placeholder="Email-Id" class="input" autocomplete="false"><br><br>
    <input type="text" name="address1" placeholder="Address 1" class="input" autocomplete="false"><br><br>
    <input type="text" name="address2" placeholder="Address 2" class="input" autocomplete="false"><br><br>
    <input type="text" name="city" placeholder="City" class="input" autocomplete="false"><br><br>
    <input type="text" name="colonyName" placeholder="Colony Name" class="input" autocomplete="false"><br><br>
    <input type="text" name="shift" placeholder="shift" class="input" autocomplete="false"><br><br>
