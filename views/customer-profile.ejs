    <input type="text" name="minBalance" placeholder="Min Balance" class="input" autocomplete="false"><br><br>
    <input type="text" name="accBalance" placeholder="Account Balance" class="input" autocomplete="false"><br><br>
    <input type="text" name="cardNumberType" placeholder="Card Number Type" class="input" autocomplete="false"><br><br>
    <input type="submit" id="submit" class="btn-grad1" value="REGISTER">
